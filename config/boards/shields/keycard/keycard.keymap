#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
    &kp LSHIFT &kp Q    &kp UP   &kp F
    &kp LCTRL  &kp LEFT &kp DOWN &kp RIGHT
                                 &kp E     &kp SPACE
            >;
        };
    };
};